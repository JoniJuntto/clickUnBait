{
  "manifest_version": 3,
  "name": "Tab Group Organizer",
  "version": "1.0",
  "permissions": ["tabs", "storage", "tabGroups"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.webp",
      "48": "icons/icon48.webp",
      "128": "icons/icon128.webp"
    }
  },
  "icons": {
    "16": "icons/icon16.webp",
    "48": "icons/icon48.webp",
    "128": "icons/icon128.webp"
  }
}
